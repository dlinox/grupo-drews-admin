{
  "version": 2,
  "functions": {
      "api/index.php": { "runtime": "vercel-php@0.5.2" }
  },
  "routes": [{
      "src": "/(.*)",
      "dest": "/api/index.php"
  }],
  "env": {
    "APP_NAME": "Laravel Vercel",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://laravel-vercel.vercel.app"
  }
}